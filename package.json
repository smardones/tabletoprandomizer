{
  "name": "tabletoprandomizer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "seed": "cd server && node seeds",
    "heroku-postbuild": "cd client && npm i && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smardones/tabletoprandomizer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smardones/tabletoprandomizer/issues"
  },
  "homepage": "https://github.com/smardones/tabletoprandomizer#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
